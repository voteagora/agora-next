model ForumCategory {
  id              Int                         @id @default(autoincrement())
  dao_slug        DaoSlug
  name            String
  description     String?
  archived        Boolean                     @default(false)
  adminOnlyTopics Boolean                     @default(false) @map("admin_only_topics")
  isDuna          Boolean                     @default(false) @map("is_duna")
  createdAt       DateTime                    @default(now()) @map("created_at")
  updatedAt       DateTime                    @updatedAt @map("updated_at")
  topics          ForumTopic[]
  attachments     ForumCategoryAttachment[]
  subscriptions   ForumCategorySubscription[]

  @@unique([dao_slug, name])
  @@index([dao_slug])
  @@map("forum_categories")
  @@schema("alltenant")
}

model ForumTopic {
  id             Int                      @id @default(autoincrement())
  dao_slug       DaoSlug
  title          String
  address        String
  categoryId     Int?                     @map("category_id")
  category       ForumCategory?           @relation(fields: [categoryId], references: [id])
  postsCount     Int                      @default(0) @map("posts_count")
  archived       Boolean                  @default(false)
  deletedAt      DateTime?                @map("deleted_at")
  deletedBy      String?                  @map("deleted_by")
  isNsfw         Boolean                  @default(false) @map("is_nsfw")
  createdAt      DateTime                 @default(now()) @map("created_at")
  updatedAt      DateTime                 @updatedAt @map("updated_at")
  posts          ForumPost[]
  subscriptions  ForumTopicSubscription[]
  viewStats      ForumTopicViewStats[]

  @@index([dao_slug])
  @@index([dao_slug, categoryId])
  @@map("forum_topics")
  @@schema("alltenant")
}

model ForumPost {
  id             Int                   @id @default(autoincrement())
  dao_slug       DaoSlug
  topicId        Int                   @map("topic_id")
  topic          ForumTopic            @relation(fields: [topicId], references: [id])
  address        String
  parentPostId   Int?                  @map("parent_post_id")
  parentPost     ForumPost?            @relation("PostToPost", fields: [parentPostId], references: [id])
  replies        ForumPost[]           @relation("PostToPost")
  content        String
  deletedAt      DateTime?             @map("deleted_at")
  deletedBy      String?               @map("deleted_by")
  isNsfw         Boolean               @default(false) @map("is_nsfw")
  createdAt      DateTime              @default(now()) @map("created_at")
  votes          ForumPostVote[]
  reactions      ForumPostReaction[]
  attachments    ForumPostAttachment[]

  @@index([dao_slug])
  @@index([dao_slug, topicId])
  @@index([dao_slug, parentPostId])
  @@map("forum_posts")
  @@schema("alltenant")
}

model ForumPostVote {
