/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view proxies {
  address      String?
  proxy        String?
  new_balance  String?
  block_number BigInt?

  @@ignore
  @@schema("center")
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view proxies_snaps {
  address      String?
  proxy        String?
  new_balance  String?
  block_number BigInt?

  @@ignore
  @@schema("center")
