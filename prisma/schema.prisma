  UNI
  NOUNS
  LYRA
  ETHERFI

  @@map("dao_slug")
  @@schema("config")
}

enum Chain {
  optimism_mainnet @map("optimism-mainnet")
  ethereum_mainnet @map("ethereum-mainnet")

  @@map("chain")
  @@schema("config")
}

enum ContractType {
  governor
  token
  alligator
  proposal_types_configurator

  @@map("contract_type")
  @@schema("config")
}

